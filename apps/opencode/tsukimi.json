{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "background": "#0d0e12",
    "backgroundAlt": "#0a0b0f",
    "surface": "#161a22",
    "button": "#252A36",
    "hover": "#3a3f4a",
    "text": "#f0ebe5",
    "textMuted": "#b8b3aa",
    "textDim": "#8a8a90",
    "textVeryDim": "#6a6f7a",
    "accent": "#cdb195",
    "accentBright": "#e5d5b8",
    "accentDim": "#b8a489",
    "blue": "#6a7a9a",
    "blueBright": "#8a9fba",
    "cyan": "#7a8a9a",
    "cyanBright": "#9aafca",
    "green": "#8a9a80",
    "greenBright": "#aac098",
    "red": "#8f6a5a",
    "redBright": "#aa7a6a",
    "orange": "#aa7a6a",
    "magenta": "#9a7a8a",
    "magentaBright": "#ba9aaa"
  },
  "theme": {
    "primary": {
      "dark": "accent",
      "light": "accent"
    },
    "secondary": {
      "dark": "blue",
      "light": "blueBright"
    },
    "accent": {
      "dark": "accent",
      "light": "accentBright"
    },
    "error": {
      "dark": "redBright",
      "light": "red"
    },
    "warning": {
      "dark": "accentBright",
      "light": "accent"
    },
    "success": {
      "dark": "greenBright",
      "light": "green"
    },
    "info": {
      "dark": "blueBright",
      "light": "blue"
    },
    "text": {
      "dark": "text",
      "light": "background"
    },
    "textMuted": {
      "dark": "textMuted",
      "light": "button"
    },
    "background": {
      "dark": "background",
      "light": "#f5f0e8"
    },
    "backgroundPanel": {
      "dark": "surface",
      "light": "#faf7f2"
    },
    "backgroundElement": {
      "dark": "button",
      "light": "#e8dfd0"
    },
    "border": {
      "dark": "button",
      "light": "#e8dfd0"
    },
    "borderActive": {
      "dark": "accent",
      "light": "accent"
    },
    "borderSubtle": {
      "dark": "surface",
      "light": "#faf7f2"
    },
    "diffAdded": {
      "dark": "greenBright",
      "light": "green"
