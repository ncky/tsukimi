    "diffRemoved": {
      "dark": "redBright",
      "light": "red"
    },
    "diffContext": {
      "dark": "textDim",
      "light": "textMuted"
    },
    "diffHunkHeader": {
      "dark": "textDim",
      "light": "button"
    },
    "diffHighlightAdded": {
      "dark": "greenBright",
      "light": "green"
    },
    "diffHighlightRemoved": {
      "dark": "redBright",
      "light": "red"
    },
    "diffAddedBg": {
      "dark": "#1a1e1c",
      "light": "#e8f0e8"
    },
    "diffRemovedBg": {
      "dark": "#1c1614",
      "light": "#f0e8e8"
    },
    "diffContextBg": {
      "dark": "surface",
      "light": "#faf7f2"
    },
    "diffLineNumber": {
      "dark": "textVeryDim",
      "light": "textMuted"
    },
    "diffAddedLineNumberBg": {
      "dark": "#1a1e1c",
      "light": "#e8f0e8"
    },
    "diffRemovedLineNumberBg": {
      "dark": "#1c1614",
      "light": "#f0e8e8"
    },
    "markdownText": {
      "dark": "text",
      "light": "#4a3f35"
    },
    "markdownHeading": {
      "dark": "accent",
      "light": "accent"
    },
    "markdownLink": {
      "dark": "blueBright",
      "light": "blue"
    },
    "markdownLinkText": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownCode": {
      "dark": "greenBright",
      "light": "green"
    },
    "markdownBlockQuote": {
      "dark": "textDim",
      "light": "textMuted"
    },
    "markdownEmph": {
      "dark": "magentaBright",
      "light": "magenta"
    },
    "markdownStrong": {
      "dark": "accentBright",
      "light": "accent"
    },
    "markdownHorizontalRule": {
      "dark": "button",
      "light": "#e8dfd0"
    },
    "markdownListItem": {
      "dark": "blueBright",
      "light": "blue"
    },
    "markdownListEnumeration": {
      "dark": "accent",
      "light": "accent"
    },
    "markdownImage": {
      "dark": "magentaBright",
      "light": "magenta"
    },
    "markdownImageText": {
      "dark": "cyan",
      "light": "cyan"
    },
    "markdownCodeBlock": {
      "dark": "text",
      "light": "#4a3f35"
    },
    "syntaxComment": {
      "dark": "textVeryDim",
      "light": "#6b5d4f"
    },
    "syntaxKeyword": {
      "dark": "redBright",
      "light": "#c17a5c"
    },
    "syntaxFunction": {
      "dark": "magentaBright",
      "light": "#a67f69"
    },
    "syntaxVariable": {
      "dark": "text",
      "light": "#4a3f35"
    },
    "syntaxString": {
      "dark": "greenBright",
      "light": "#a68968"
    },
    "syntaxNumber": {
      "dark": "accentBright",
      "light": "#e6be8f"
    },
    "syntaxType": {
      "dark": "blueBright",
      "light": "#7a8ca3"
    },
    "syntaxOperator": {
      "dark": "cyan",
      "light": "#9b8a73"
    },
    "syntaxPunctuation": {
      "dark": "textMuted",
      "light": "#6b5d4f"
    }
  }
}
